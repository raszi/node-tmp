{
  "opts": {
    "readme": "./README.md",
    "destination": "docs",
    "tutorials": "./tutorials"
  },
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.ts$"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "default": {
      "outputSourceFiles": false,
      "readme": "README.md"
    }
  }
}
